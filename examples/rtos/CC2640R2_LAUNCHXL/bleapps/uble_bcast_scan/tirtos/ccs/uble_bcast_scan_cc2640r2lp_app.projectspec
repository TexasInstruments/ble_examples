        <file path="SRC_BLE_DIR/common/cc26xx/util.c" openOnCreation="" excludeFromBuild="false" action="copy" targetDirectory="Application" createVirtualFolders="true"/>
        <file path="SRC_BLE_DIR/common/cc26xx/util.h" openOnCreation="" excludeFromBuild="false" action="copy" targetDirectory="Application" createVirtualFolders="true"/>

        <!-- HAL/Common Folder -->
        <file path="SRC_BLE_DIR/hal/src/common/hal_assert.c" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="HAL/Common" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>

        <!-- HAL/Include Folder -->
        <file path="SRC_BLE_DIR/hal/src/inc/hal_assert.h" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="HAL/Include" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>

        <!-- Stack Folder -->
        <file path="SRC_BLE_DIR/microstack/port.c" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Stack" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
        <file path="SRC_BLE_DIR/inc/port.h" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Stack" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
        <file path="SRC_BLE_DIR/microstack/uble.c" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Stack" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
        <file path="SRC_BLE_DIR/inc/uble.h" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Stack" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
        <file path="SRC_BLE_DIR/microstack/ugap.c" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Stack" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
