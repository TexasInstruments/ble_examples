        <file path="EXAMPLE_BLE_ROOT/src/app/uble_bcast_scan.c" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Application" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
        <file path="EXAMPLE_BLE_ROOT/src/app/uble_bcast_scan.h" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Application" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>
