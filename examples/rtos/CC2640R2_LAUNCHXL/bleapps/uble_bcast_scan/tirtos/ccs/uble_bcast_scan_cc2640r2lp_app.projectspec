        <file path="SRC_BLE_DIR/inc/urfi.h" openOnCreation="" excludeFromBuild="false" action="link" targetDirectory="Stack" createVirtualFolders="true" applicableConfigurations="MultiMode_FlashOnly"/>

        <!-- Startup Folder -->
